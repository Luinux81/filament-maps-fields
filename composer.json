{
    "name": "lbcdev/filament-maps-fields",
    "description": "A Filament package providing map field components for forms and infolists using lbcdev/livewire-maps-core Livewire component",
    "keywords": [
        "filament",
        "laravel",
        "map",
        "field",
        "coordinates",
        "leaflet",
        "infolist",
        "form",
        "livewire"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LBCDev",
            "email": "luinux81@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "filament/filament": "^3.0|^4.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "lbcdev/livewire-maps-core": "dev-main",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../packages/core",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "LBCDev\\FilamentMapsFields\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LBCDev\\FilamentMapsFields\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LBCDev\\FilamentMapsFields\\FilamentMapsFieldsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}